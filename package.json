    "jsxLintFix" :" eslint \\\"**/*.{js,jsx}\\\" --fix\"",
    "nvm_use": "chmod +x scripts/build.sh && scripts/build.sh",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
